

@kernel void ellipticFooTri2D(const dlong Nelements,
			      @restrict const  dfloat *  Pq,
			      @restrict dfloat *  Rq){
  
  
  for(dlong e=0;e<Nelements;++e;@outer(0)){
    for(int n=0;n<p_Np;++n;@inner(0)){
      dlong idP = n + e*p_NpP;	
      dlong id  = n + e*p_Np;
      Rq[id] = Pq[idP];
    }
  }
}
