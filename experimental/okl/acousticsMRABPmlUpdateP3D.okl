        s_wn[n] = s_q[qidM+2];
        s_pn[n] = s_q[qidM+3];

        //write negative traces
        fQM[qid+0] = s_un[n];
        fQM[qid+1] = s_vn[n];
        fQM[qid+2] = s_wn[n];
        fQM[qid+3] = s_pn[n];
      }
    }

    @barrier("local");    

    for(int n=0; n < p_maxNodes; ++n; @inner(0)){        
      if (n < p_Nfp*p_Nfaces){
