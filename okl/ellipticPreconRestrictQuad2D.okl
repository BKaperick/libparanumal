

@kernel void ellipticFooQuad2D(const int Nelements,
			      @restrict const  dfloat *  Pq,
			      @restrict dfloat *  Rq){
  
  
  for(int e=0;e<Nelements;++e;@outer(0)){
    for(int j=0;j<p_Nq;++j;@inner(1)){
      for(int i=0;i<p_Nq;++i;@inner(0)){
		int idP = (i+1) + (p_NqP*(j+1)) + e*(p_NpP);	
		int id  = i   + p_Nq*j + e*p_Np;
		  Rq[id] = Pq[idP];
		  //	  Rq[id] = 1;
      }
    }
  }
}
