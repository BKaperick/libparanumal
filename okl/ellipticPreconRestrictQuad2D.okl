

kernel void ellipticFooQuad2D(const int Nelements,
			      const dfloat * restrict Pq,
			      dfloat * restrict Rq){
  
  
  for(int e=0;e<Nelements;++e;outer0){
    for(int j=0;j<p_Nq;++j;inner1){
      for(int i=0;i<p_Nq;++i;inner0){
		int idP = (i+1) + (p_NqP*(j+1)) + e*(p_NpP);	
		int id  = i   + p_Nq*j + e*p_Np;
		  Rq[id] = Pq[idP];
		  //	  Rq[id] = 1;
      }
    }
  }
}
