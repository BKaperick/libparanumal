kernel void multiScaledAdd(const int L,
					            const int N,
                      const dfloat * restrict alpha,
                      const dfloat * restrict x,
                      const dfloat beta,
                      dfloat * restrict y){
  
  for(int n=0;n<N;++n;tile(256)){
    if(n<N){
      for (int l=0;l<L;l++) {
      	y[n] = alpha[l]*x[n+l*N] + beta*y[n];	
      }
    }
  }
}