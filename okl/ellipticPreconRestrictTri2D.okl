

kernel void ellipticFooTri2D(const dlong Nelements,
			      const dfloat * restrict Pq,
			      dfloat * restrict Rq){
  
  
  for(dlong e=0;e<Nelements;++e;outer0){
    for(int n=0;n<p_Np;++n;inner0){
      dlong idP = n + e*p_NpP;	
      dlong id  = n + e*p_Np;
      Rq[id] = Pq[idP];
    }
  }
}
